<div class="flex flex-col w-full space-y-2">
  {#for intent <- @intents}
    <Bonfire.UI.Breadpub.IntentLive
      key={e(intent, :id)}
      name={e(intent, :name, nil)}
      note={e(intent, :note, nil)}
      intent={intent}
      current_user={current_user(@__context__)}
      />
  {#else}
    No items
  {/for}
</div>
